---
import Technology from './Technology.astro';
import { SkillsIcons } from '../constans/SkillsIcons';
---

<section id="skills">
  <h3 class="text-3xl md:text-4xl font-extrabold text-primary-900 mb-10">MIS SKILLS</h3>
  <ul class="grid grid-cols-2 lg:grid-cols-6 gap-3">
    {SkillsIcons.map(({ name, Icon }) => <Technology Icon={Icon} name={name} />)}
  </ul>
</section>
